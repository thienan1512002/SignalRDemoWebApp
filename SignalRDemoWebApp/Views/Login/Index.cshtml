@using Microsoft.AspNetCore.Http
@model IEnumerable<Notyf>
@{
    ViewData["Title"] = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h1>Index</h1>
<div class="row">
    
    <div class="col-6">
        <input type="button" id="btnCheckin" class="btn btn-success" value="Check in"/>
    </div>
    <div class="col-6">
        <input type="button" id="btnCheckout" class="btn btn-success" value="Check out"/>
    </div>
   
</div>
<div class="row">
    <div class="col-6">
        <h3 align="center">Check in Lists :</h3>
        <ul id="userList"></ul>
    </div>
    <div class="col-6">
        <h3 align="center">Check out Lists :</h3>
        <ul id="logList"></ul>
    </div>
</div>
<div class="row">
    <h3 align="center">Log Lists Load from SQL Server :</h3>
    <ul id="dataList"></ul>
</div>
    <input type="hidden" value=@System.DateTime.Now.ToString("dd'/'MM'/'yyyy HH:mm:ss") id="time" />



<script src="~/js/signalr/dist/browser/signalr.js"></script>
 <script src="~/lib/jquery/dist/jquery.min.js"></script>
<script src="~/js/user.js"></script>

